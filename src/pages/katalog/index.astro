---
export const prerender = false; // SSR mode enabled for page

import Container from "../../components/Container.astro";
import RootLayout from "../../layouts/RootLayout.astro";
import CatalogHeader from "../../components/pages/catalog/CatalogHeader.astro";
import CatalogContent from "../../components/pages/catalog/CatalogContent.astro";
import CallToAction from "@components/CallToAction.astro";
import WaveIntersection from "@components/pages/root/intersection/WaveIntersection.astro";
---

<RootLayout title="Aqua Aria | Katalog">
  <Fragment>
    <Container className="py-14 md:py-18 lg:py-24">
      <Fragment>
        <CatalogHeader />
        <CatalogContent />
      </Fragment>
    </Container>
    <WaveIntersection />
    <CallToAction />
  </Fragment>
</RootLayout>
