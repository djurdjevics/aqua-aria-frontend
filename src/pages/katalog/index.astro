---

export const prerender = false; // SSR mode enabled for page

import { WaveSecondaryMobile } from "../../assets/waves/WaveSecondary";
import {CallToAction} from "../../components/CallToAction";
import Container from "../../components/Container.astro";
import RootLayout from "../../layouts/RootLayout.astro";
import CatalogHeader from "../../components/pages/catalog/CatalogHeader.astro";
import CatalogContent from "../../components/pages/catalog/CatalogContent.astro";
---

<RootLayout title="Aqua Aria | Katalog">
  <Fragment>
    <div class="relative pb-32">
      <Container className="py-14 md:py-18 lg:py-24">
        <Fragment>
          <CatalogHeader />
          <CatalogContent />
        </Fragment>
      </Container>
      <WaveSecondaryMobile />
    </div>
    <CallToAction />
  </Fragment>
</RootLayout>
