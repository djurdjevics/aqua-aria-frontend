---
import RootLayout from "../layouts/RootLayout.astro";
import HeroSection from "@components/pages/root/hero/HeroSection.astro";
import DispenserSection from "@components/pages/root/dispenser/DispenserSection.astro";
import OurMissionSection from "@components/pages/root/our-mission/OurMissionSection.astro";
import ServicesSection from "@components/pages/root/services/ServicesSection.astro";
import WatersSection from "@components/pages/root/waters/WatersSection.astro";
import AnanasSection from "../components/pages/root/ananas/AnanasSection.astro";
import { TestimonialsSection } from "../components/pages/root/testimonials/TestimonialsSection";
import { CallToAction } from "../components/CallToAction";
import WaveIntersection from "@components/pages/root/intersection/WaveIntersection.astro";
---

<RootLayout title="Aqua Aria | Početna">
  <HeroSection />
  <WaveIntersection />
  <DispenserSection />
  <OurMissionSection />
  <ServicesSection />
  <WatersSection />
  <AnanasSection />
  <TestimonialsSection client:only="react" />
  <CallToAction />
</RootLayout>
